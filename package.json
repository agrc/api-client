{
  "name": "ugrc-api-client",
  "productName": "ugrc-api-client",
  "version": "1.7.7",
  "description": "The official UGRC API client",
  "license": "MIT",
  "type": "module",
  "repository": "https://github.com/agrc/api-client",
  "homepage": "https://gis.utah.gov/products/sgid/address/api-client/",
  "bugs": "https://github.com/agrc/api-client/issues",
  "contributors": [
    {
      "name": "Steve Gourley",
      "email": "sgourley@utah.gov"
    },
    {
      "name": "Scott Davis",
      "email": "stdavis@utah.gov"
    }
  ],
  "keywords": [
    "utah",
    "geocoding",
    "ugrc",
    "agrc",
    "electron"
  ],
  "scripts": {
    "start": "cross-env ELECTRON_ENABLE_LOGGING=1 electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "format": "prettier --write src --ignore-path ./.gitignore",
    "format:fix": "prettier --loglevel warn --write \"./**/*.{js,jsx,css,md,json}\" --ignore-path ./.gitignore",
    "fix": "npm run format:fix && npm run lint:fix",
    "lint": "eslint ./src --ext .jsx,.js --ignore-path ./.gitignore",
    "lint:fix": "eslint ./src --ext .jsx,.js --quiet --fix --ignore-path ./.gitignore",
    "upload-debug-symbols": "node sentry-symbols.js"
  },
  "main": ".vite/build/main.js",
  "config": {
    "forge": "./forge.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.25.8",
    "@babel/eslint-parser": "^7.25.8",
    "@babel/preset-env": "^7.25.8",
    "@babel/preset-react": "^7.25.7",
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-dmg": "^7.5.0",
    "@electron-forge/maker-squirrel": "^7.5.0",
    "@electron-forge/maker-zip": "^7.5.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.5.0",
    "@electron-forge/plugin-fuses": "^7.5.0",
    "@electron-forge/plugin-vite": "^7.5.0",
    "@electron-forge/publisher-github": "^7.5.0",
    "@electron/fuses": "^1.8.0",
    "@sentry/cli": "^2.36.4",
    "@tailwindcss/forms": "^0.5.9",
    "@vitejs/plugin-react": "^4.3.2",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.5",
    "electron": "^33.0.1",
    "electron-download": "^4.1.1",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "nock": "^13.5.5",
    "postcss": "^8.4.47",
    "postcss-loader": "^8.1.1",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.14",
    "vite": "^5.4.9",
    "vite-plugin-static-copy": "^1.0.6"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.8",
    "@heroicons/react": "^2.1.5",
    "@sentry/electron": "^5.4.0",
    "@sentry/react": "^8.32.0",
    "assert": "^2.1.0",
    "clsx": "^2.1.1",
    "crypto-browserify": "^3.12.0",
    "csv-parse": "^5.5.6",
    "csv-stringify": "^6.5.1",
    "electron-google-analytics4": "^1.2.0",
    "electron-log": "^5.2.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.0.0",
    "electron-unhandled": "^5.0.0",
    "electron-util": "^0.18.1",
    "electron-window-state": "^5.0.3",
    "file-loader": "^6.2.0",
    "humanize-duration": "^3.32.1",
    "immer": "^10.1.1",
    "ky": "^1.7.2",
    "md5": "^2.3.0",
    "node-machine-id": "^1.1.12",
    "os-browserify": "^0.3.0",
    "os-name": "^6.0.0",
    "path-browserify": "^1.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^4.0.13",
    "react-navigator-status": "^0.1.2",
    "react-router-dom": "^5.3.4",
    "stream-browserify": "^3.0.0",
    "update-electron-app": "^3.0.0",
    "use-immer": "^0.10.0",
    "util": "^0.12.5"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "env": {
      "browser": true,
      "node": true,
      "es6": true
    },
    "globals": {
      "MAIN_WINDOW_VITE_DEV_SERVER_URL": "readonly",
      "MAIN_WINDOW_VITE_NAME": "readonly"
    },
    "rules": {
      "react/jsx-uses-react": "off",
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off",
      "jsx-a11y/no-onchange": "off",
      "prettier/prettier": "off"
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended",
      "plugin:react-hooks/recommended",
      "plugin:jsx-a11y/recommended",
      "plugin:prettier/recommended",
      "prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "printWidth": 120,
    "semi": true
  },
  "browserslist": [
    "electron >= 15.1.1"
  ],
  "babel": {
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env",
          "@babel/preset-react"
        ]
      }
    }
  }
}

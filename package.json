{
  "name": "ugrc-api-client",
  "version": "1.7.8",
  "description": "The official UGRC API client",
  "keywords": [
    "utah",
    "geocoding",
    "ugrc",
    "agrc",
    "electron"
  ],
  "homepage": "https://gis.utah.gov/products/sgid/address/api-client/",
  "bugs": "https://github.com/agrc/api-client/issues",
  "repository": "https://github.com/agrc/api-client",
  "license": "MIT",
  "contributors": [
    {
      "name": "Steve Gourley",
      "email": "sgourley@utah.gov"
    },
    {
      "name": "Scott Davis",
      "email": "stdavis@utah.gov"
    }
  ],
  "main": ".vite/build/main.js",
  "scripts": {
    "fix": "npm run format:fix && npm run lint:fix",
    "format": "prettier --write src --ignore-path ./.gitignore",
    "format:fix": "prettier --loglevel warn --write \"./**/*.{ts,tsx,js,jsx,css,md,json}\" --ignore-path ./.gitignore",
    "lint": "eslint ./src --ext .ts,.tsx,.jsx,.js --ignore-path ./.gitignore",
    "lint:fix": "eslint ./src --ext .ts,.tsx,.jsx,.js --quiet --fix --ignore-path ./.gitignore",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "start": "cross-env ELECTRON_ENABLE_LOGGING=1 electron-forge start",
    "upload-debug-symbols": "node sentry-symbols.js"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.3",
    "clsx": "^2.1.1",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.5.2",
    "electron-log": "^5.4.0",
    "electron-store": "^10.0.1",
    "electron-unhandled": "^5.0.0",
    "electron-util": "^0.18.1",
    "electron-window-state": "^5.0.3",
    "humanize-duration": "^3.32.2",
    "ky": "^1.8.1",
    "md5": "^2.3.0",
    "os-name": "^6.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.2.10",
    "react-error-boundary": "^6.0.0",
    "react-navigator-status": "^0.1.2",
    "react-router-dom": "^5.3.4",
    "update-electron-app": "^3.1.1",
    "use-immer": "^0.11.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.1",
    "@electron-forge/maker-deb": "^7.8.1",
    "@electron-forge/maker-dmg": "^7.8.1",
    "@electron-forge/maker-rpm": "^7.8.1",
    "@electron-forge/maker-squirrel": "^7.8.1",
    "@electron-forge/maker-zip": "^7.8.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.1",
    "@electron-forge/plugin-fuses": "^7.8.1",
    "@electron-forge/plugin-vite": "^7.8.1",
    "@electron-forge/publisher-github": "^7.8.1",
    "@electron/fuses": "^1.8.0",
    "@heroicons/react": "^2.2.0",
    "@tailwindcss/vite": "^4.1.6",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/react": "^19.1.4",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "cross-env": "^7.0.3",
    "dotenv": "^16.5.0",
    "electron": "^36.2.0",
    "electron-squirrel-startup": "^1.0.1",
    "eslint": "^9.26.0",
    "eslint-plugin-import": "^2.31.0",
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.12",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.1.6",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "vite": "^6.3.5"
  },
  "productName": "ugrc-api-client"
}

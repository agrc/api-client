{
  "name": "ugrc-api-client",
  "version": "1.7.12",
  "description": "The official UGRC API client",
  "keywords": [
    "utah",
    "geocoding",
    "ugrc",
    "agrc",
    "electron"
  ],
  "homepage": "https://gis.utah.gov/products/sgid/address/api-client/",
  "bugs": "https://github.com/agrc/api-client/issues",
  "repository": "https://github.com/agrc/api-client",
  "license": "MIT",
  "contributors": [
    {
      "name": "Steve Gourley",
      "email": "sgourley@utah.gov"
    },
    {
      "name": "Scott Davis",
      "email": "stdavis@utah.gov"
    }
  ],
  "main": ".vite/build/main.js",
  "scripts": {
    "prebuild": "generate-license-file --config glf.json",
    "build": "npm run prebuild && tsc -b && vite build",
    "check": "tsc -b --noEmit",
    "format": "prettier . --write",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "make": "electron-forge make",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "start": "cross-env ELECTRON_ENABLE_LOGGING=1 electron-forge start"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "clsx": "^2.1.1",
    "csv-parse": "^6.1.0",
    "csv-stringify": "^6.6.0",
    "electron-log": "^5.4.3",
    "electron-store": "^11.0.2",
    "electron-unhandled": "^5.0.0",
    "electron-util": "^1.0.0",
    "electron-window-state": "^5.0.3",
    "humanize-duration": "^3.33.2",
    "ky": "^1.14.3",
    "lucide-react": "^0.563.0",
    "md5": "^2.3.0",
    "os-name": "^6.1.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-dropzone": "14.2",
    "react-error-boundary": "^6.1.0",
    "react-navigator-status": "^0.1.2",
    "react-router": "^7.13.0",
    "update-electron-app": "^3.1.2",
    "use-immer": "^0.11.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.11.1",
    "@electron-forge/maker-deb": "^7.11.1",
    "@electron-forge/maker-dmg": "^7.11.1",
    "@electron-forge/maker-rpm": "^7.11.1",
    "@electron-forge/maker-squirrel": "^7.11.1",
    "@electron-forge/maker-zip": "^7.11.1",
    "@electron-forge/plugin-auto-unpack-natives": "^7.11.1",
    "@electron-forge/plugin-fuses": "^7.11.1",
    "@electron-forge/plugin-vite": "^7.11.1",
    "@electron-forge/publisher-github": "^7.11.1",
    "@electron/fuses": "v1.8",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@ugrc/eslint-config": "^1.2.3",
    "chokidar": "^5.0.0",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.2",
    "electron": "^39.2.7",
    "electron-squirrel-startup": "^1.0.1",
    "eslint": "^9.32.0",
    "generate-license-file": "^4.1.1",
    "prettier": "^3.8.1",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-packagejson": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.3",
    "vite": "^7.3.1"
  },
  "productName": "ugrc-api-client"
}
